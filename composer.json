{
    "name": "jmalko/afterschool-statamic",
    "license": "proprietary",
    "type": "statamic-addon",
    "description": "A learning management system for Statamic CMS",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^8.40",
        "statamic/cms": "^3.3"
    },

    "autoload": {
        "psr-4": {
            "Jmalko\\AfterschoolStatamic\\": "src"
        }
    },
    "extra": {
        "statamic": {
            "name": "Afterschool Statamic",
            "description": "Afterschool Statamic addon"
        },
        "laravel": {
            "providers": [
                "Jmalko\\AfterschoolStatamic\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pixelfear/composer-dist-plugin": true
        }
    }
}
